<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.elect.dao.ItemDao">
    <select id="addItems" parameterType="Item" >
        insert into d_item(product_id,product_name,dang_price,product_num,amount,order_id)
            values(#{product_id},#{product_name},#{dang_price},#{product_num},#{amount},#{order_id})
    </select>
</mapper>